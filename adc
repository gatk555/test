S_0x55cce8b80510 .scope module, "adc" "adc" 2 3;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "Clk";
    .port_info 1 /INPUT 1 "Comp";
    .port_info 2 /INPUT 1 "Start";
    .port_info 3 /OUTPUT 1 "Sample";
    .port_info 4 /OUTPUT 1 "Done";
    .port_info 5 /OUTPUT 6 "Result";
P_0x55cce8bb77b0 .param/l "Bits" 0 2 8, +C4<00000000000000000000000000000110>;
o0x7fe32583a018 .functor BUFZ 1, c4<z>; HiZ drive
v0x55cce8b7f090_0 .net "Clk", 0 0, o0x7fe32583a018;  0 drivers
o0x7fe32583a048 .functor BUFZ 1, c4<z>; HiZ drive
v0x55cce8bccc70_0 .net "Comp", 0 0, o0x7fe32583a048;  0 drivers
v0x55cce8bccd30_0 .var "Done", 0 0;
v0x55cce8bcce00_0 .var "Result", 5 0;
v0x55cce8bccee0_0 .var "Running", 0 0;
v0x55cce8bccff0_0 .var "SR", 5 0;
v0x55cce8bcd0d0_0 .var "Sample", 0 0;
o0x7fe32583a168 .functor BUFZ 1, c4<z>; HiZ drive
v0x55cce8bcd190_0 .net "Start", 0 0, o0x7fe32583a168;  0 drivers
E_0x55cce8bb7ab0 .event posedge, v0x55cce8b7f090_0;
    .scope S_0x55cce8b80510;
T_0 ;
    %vpi_call 2 14 "$display", "ADC simulation starting" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cce8bccd30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cce8bcd0d0_0, 0, 1;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x55cce8bcce00_0, 0, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cce8bccee0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55cce8b80510;
T_1 ;
    %wait E_0x55cce8bb7ab0;
    %load/vec4 v0x55cce8bccee0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55cce8bcd0d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cce8bcd0d0_0, 0;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 5, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55cce8bccff0_0, 4, 5;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 5, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55cce8bcce00_0, 4, 5;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55cce8bccff0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v0x55cce8bccc70_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.6, 8;
    %load/vec4 v0x55cce8bcce00_0;
    %load/vec4 v0x55cce8bccff0_0;
    %inv;
    %and;
    %jmp/1 T_1.7, 8;
T_1.6 ; End of true expr.
    %load/vec4 v0x55cce8bcce00_0;
    %jmp/0 T_1.7, 8;
 ; End of false expr.
    %blend;
T_1.7;
    %load/vec4 v0x55cce8bccff0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %or;
    %assign/vec4 v0x55cce8bcce00_0, 0;
    %load/vec4 v0x55cce8bccff0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %assign/vec4 v0x55cce8bccff0_0, 0;
    %load/vec4 v0x55cce8bccff0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.8, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cce8bccee0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55cce8bccd30_0, 0;
T_1.8 ;
T_1.4 ;
T_1.3 ;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55cce8bcd190_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.10, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55cce8bccee0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55cce8bcd0d0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cce8bccd30_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x55cce8bccff0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x55cce8bcce00_0, 0;
T_1.10 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
